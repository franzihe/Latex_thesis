@Article{wolff_measurements_2013,
  Title                    = {Measurements of wind-induced loss of solid precipitation: description of a {Norwegian} field study},
  Author                   = {Wolff, M. A. and Isaksen, K. and Brækkan, R. and Alfnes, E. and Petersen-Øverleir, A. and Ruud, E},
