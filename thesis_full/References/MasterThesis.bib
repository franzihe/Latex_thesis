@Article{wood_microphysical_2015,
  Title                    = {Microphysical {Constraints} on {Millimeter}-{Wavelength} {Scattering} {Properties} of {Snow} {Particles}},
  Author                   = {Wood, N. B. and L’Ecuyer, T. S. and Heymsfield, A. J. and Stephens, G. L.},
