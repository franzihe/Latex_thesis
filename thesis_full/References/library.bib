@Article{wolff2013,
  Title                    = {Measurements of wind-induced loss of solid precipitation: description of a {Norwegian} field study},
  Author                   = {Wolff, Mareile and Isaksen, Ketil and Brækkan, Ragnar and Alfnes, Eli and Petersen-Øverleir, Asgeir and Ruud, Erik},
